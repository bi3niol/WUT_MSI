<Page x:Class="WUT_MSI.MainApp.Start"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WUT_MSI.MainApp"
      mc:Ignorable="d" 
      d:DesignHeight="350" d:DesignWidth="525"
      Title="Start">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition Height="2*"/>
        </Grid.RowDefinitions>
        <Label Content="MSI - Rozmytość" Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <Button Content="Start" VerticalAlignment="Center" Width="100" Click="Start_Click" Grid.Row="2" Padding="5"/>
    </Grid>
</Page>
